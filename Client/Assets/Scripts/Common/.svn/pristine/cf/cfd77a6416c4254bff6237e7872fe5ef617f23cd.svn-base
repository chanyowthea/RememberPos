using System.Collections;
using System.Collections.Generic;
using ProtoBuf; 

[ProtoContract]
public class BaseMessage
{
}

// 登录流程0x01
[ProtoContract]
public class C_LoginMessage : BaseMessage
{
	public static int _msgId = 0x0101;
	[ProtoMember(1)]
	public string _accountName;
}

[ProtoContract]
public class S_LoginMessage : BaseMessage
{
	public static int _msgId = 0x0102;
	[ProtoMember(1)]
	public byte[] _playerData;
}

// 客户端退出游戏或者点击登出
[ProtoContract]
public class C_LogoutMessage : BaseMessage
{
	public static int _msgId = 0x0103;
	[ProtoMember(1)]
	public string _accountName;
}

// 服务器强制踢出（同名帐号、作弊等）
[ProtoContract]
public class S_LogoutMessage : BaseMessage
{
	public static int _msgId = 0x0104;
}

[ProtoContract]
public class C_GetRoomListMessage : BaseMessage
{
	public static int _msgId = 0x0105;
	[ProtoMember(1)]
	public string _accountName;
}

[ProtoContract]
public class S_GetRoomListMessage : BaseMessage
{
	public static int _msgId = 0x0106;
	[ProtoMember(1)]
	public List<byte[]> _list;
}

[ProtoContract]
public class C_CreateRoomMessage : BaseMessage
{
	public static int _msgId = 0x0107;
	[ProtoMember(1)]
	public string _roomInfo;
	//[ProtoMember(2)]
	//public string _accountName;
}

[ProtoContract]
public class C_DestroyRoomMessage : BaseMessage
{
	public static int _msgId = 0x0109;
	[ProtoMember(1)]
	public byte[] _roomInfo;
	[ProtoMember(2)]
	public string _accountName;
}

